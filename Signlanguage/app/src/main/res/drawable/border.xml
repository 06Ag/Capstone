<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 상하좌우 테두리 -->
    <item
        android:bottom="1dp"
        android:left="2dp"
        android:right="2dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <!--테두리-->
            <stroke
                android:width="1dp"
                android:color="#dcdcdc" />
            <!--배경색-->
            <solid android:color="#FFF" />
        </shape>
    </item>

</layer-list>
